class Car {
    String model;
    String color;
    int year;

    // Constructor to initialize the Car object
    Car(String m, String c, int y) {
        model = m;
        color = c;
        year = y;
    }

    public static void main(String[] args) {
        // Creating an object of the Car class using the constructor
        Car car1 = new Car("Toyota", "Blue", 2022);

        // Printing the values
        System.out.println("Car Model: " + car1.model);
        System.out.println("Car Color: " + car1.color);
        System.out.println("Car Year: " + car1.year);
    }
}
